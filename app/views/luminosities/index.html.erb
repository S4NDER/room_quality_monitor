<p id="notice"><%= notice %></p>

<h1>Luminosities</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @luminosities.each do |luminosity| %>
      <tr>
        <td><%= luminosity.value %></td>
        <td><%= link_to 'Show', luminosity %></td>
        <td><%= link_to 'Edit', edit_luminosity_path(luminosity) %></td>
        <td><%= link_to 'Destroy', luminosity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Luminosity', new_luminosity_path %>
